{% extends "base.twig" %}

{% block content %}

    <h1>My Blog</h1>

    <p><a href ="/logged_in.php">Click here to go back</a></p>
    

    {% for post in posts %}
        <h2>{{ post.title }}</h2>
        <p style="text-transform: capitalize">by {{ post.author }}</p>
        <p>{{ post.body }}</p>
        
    {% endfor %}


{% endblock %}